{
  "name": "poker-hand-logger-v2",
  "version": "2.0.0",
  "description": "차세대 포커 핸드 추적 및 분석 시스템 - 완전히 재설계된 아키텍처",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --filter @poker-logger/backend dev & pnpm --filter @poker-logger/frontend dev",
    "dev:backend": "pnpm --filter @poker-logger/backend dev",
    "dev:frontend": "pnpm --filter @poker-logger/frontend dev",
    "build": "pnpm --filter @poker-logger/shared build && pnpm --filter @poker-logger/backend build && pnpm --filter @poker-logger/frontend build",
    "build:shared": "pnpm --filter @poker-logger/shared build",
    "build:backend": "pnpm --filter @poker-logger/backend build",
    "build:frontend": "pnpm --filter @poker-logger/frontend build",
    "start:backend": "pnpm --filter @poker-logger/backend start",
    "preview:frontend": "pnpm --filter @poker-logger/frontend preview",
    "prisma:generate": "pnpm --filter @poker-logger/backend prisma:generate",
    "prisma:migrate": "pnpm --filter @poker-logger/backend prisma:migrate",
    "prisma:studio": "pnpm --filter @poker-logger/backend prisma:studio",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "test": "pnpm --recursive test",
    "clean": "pnpm --recursive clean && rm -rf node_modules"
  },
  "keywords": [
    "poker",
    "hand-logger",
    "tracking",
    "analytics",
    "react",
    "typescript",
    "fastify",
    "postgresql"
  ],
  "author": "Claude & Poker Hand Logger Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/poker-hand-logger-v2.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/poker-hand-logger-v2/issues"
  },
  "homepage": "https://github.com/yourusername/poker-hand-logger-v2#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
