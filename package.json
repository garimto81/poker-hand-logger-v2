{
  "name": "poker-hand-logger-v2",
  "version": "2.0.0",
  "description": "차세대 포커 핸드 추적 및 분석 시스템 - 완전히 재설계된 아키텍처",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "pnpm --parallel --stream run dev",
    "dev:backend": "pnpm --filter @poker/backend dev",
    "dev:frontend": "pnpm --filter @poker/frontend dev",
    "build": "pnpm --recursive run build",
    "build:backend": "pnpm --filter @poker/backend build",
    "build:frontend": "pnpm --filter @poker/frontend build",
    "test": "pnpm --recursive run test",
    "test:coverage": "pnpm --recursive run test:coverage",
    "lint": "pnpm --recursive run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "clean": "pnpm --recursive run clean && rm -rf node_modules",
    "prisma:generate": "pnpm --filter @poker/backend prisma:generate",
    "prisma:migrate": "pnpm --filter @poker/backend prisma:migrate",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "poker",
    "hand-logger",
    "tracking",
    "analytics",
    "react",
    "typescript",
    "fastify",
    "postgresql"
  ],
  "author": "Claude & Poker Hand Logger Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/poker-hand-logger-v2.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/poker-hand-logger-v2/issues"
  },
  "homepage": "https://github.com/yourusername/poker-hand-logger-v2#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.15.0"
}
